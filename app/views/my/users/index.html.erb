<div class="container">
  <div class="team-members">
    <h2>My team:</h2>
      <ul>
      <% @users.each do |user| %>
        <li><%= link_to("#{user.first_name.capitalize} #{user.last_name.capitalize} | #{user.rfc.upcase}", user_path(user)) %></li>
      <% end %>
      </ul>
  </div>
  <div>
    <p><%= link_to 'Add Team Member', new_user_path, class: "btn btn-primary" %></p>
  </div>

</div>
